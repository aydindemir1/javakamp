package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {
		
		int number = 10;
		boolean asalMi = true;
		
		for(int i = 2; i < number; i++) {
			if( number % i == 0) {
				asalMi = false;
				break;
			}
		}
		if (asalMi) {
			System.out.println("sayı asaldır.");
		}else {
			System.out.println("sayı asal değildir.");
		}
		

	}

}
